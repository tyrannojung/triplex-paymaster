# Ethereum network RPC endpoint
RPC=http://127.0.0.1:8545

# Port on which the Paymaster service will run (default: 4339)
PAYMASTER_PORT=4339

# Private key used by the Paymaster for signing (replace with your actual private key)
PAYMASTER_PK=your_private_key_here

# The deployed Paymaster contract address (replace with your actual contract address)
PAYMASTER_CONTRACT=your_contract_address_here